{
  "expo": {
    "name": "navDavid",
    "slug": "navdavid",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/navdavid-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/navdavid-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gusta1996.navdavid"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e0735bf7-db30-4b5b-bc5d-937b11131b3a"
      }
    },
    "owner": "gusta1996"
  }
}
